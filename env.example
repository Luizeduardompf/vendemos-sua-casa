# Supabase Configuration
# Obtenha essas informações em: https://supabase.com/dashboard/project/[seu-projeto]/settings/api

# URL do seu projeto Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url

# Chave pública (anon key) do Supabase
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Chave de serviço (service role key) - use apenas no servidor
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Configurações do Next.js
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_key

# Configurações do projeto VendemosSuaCasa
NEXT_PUBLIC_APP_NAME=VendemosSuaCasa
NEXT_PUBLIC_APP_DESCRIPTION=Plataforma para venda de imóveis
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# Configurações de Email (opcional)
SMTP_HOST=your_smtp_host
SMTP_PORT=587
SMTP_USER=your_smtp_user
SMTP_PASS=your_smtp_password
